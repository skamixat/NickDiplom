<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.layout.VBox?>

<VBox prefHeight="400.0" prefWidth="600.0" spacing="10.0" xmlns="http://javafx.com/javafx/21" xmlns:fx="http://javafx.com/fxml/1" fx:controller="org.nikita.armaplus2.controllers.AdminController">
    <padding>
        <Insets bottom="10.0" left="10.0" right="10.0" top="10.0"/>
    </padding>
    <TableView fx:id="incomeExpenseTable" prefHeight="350.0" prefWidth="580.0">
        <columns>
            <TableColumn fx:id="typeColumn" prefWidth="150.0" text="Тип доход/расход"/>
            <TableColumn fx:id="dateColumn" prefWidth="100.0" text="Дата"/>
            <TableColumn fx:id="expenseTypeColumn" prefWidth="200.0" text="Тип"/>
            <TableColumn fx:id="amountColumn" prefWidth="130.0" text="Сумма"/>
        </columns>
    </TableView>
    <Button fx:id="backButton" onAction="#handleBackButton" text="Назад"/>
</VBox>